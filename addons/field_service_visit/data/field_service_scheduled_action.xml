<odoo>
    <!-- Scheduled Action: Tomorrow Visits -->
    <record id="ir_cron_tomorrow_visits" model="ir.cron">
        <field name="name">Tomorrow Visits Notification</field>
        <field name="model_id" ref="field_service_visit.model_field_service_visit"/>
        <field name="state">code</field>
        <field name="code">model._cron_notify_tomorrow_visits()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active">True</field>
    </record>
</odoo>
